<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.trainer.courserunner">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:name=".Application.AppFunctionLoader"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CourseRunner">
        <activity android:name=".SettingActivity"></activity>
        <activity android:name=".NewProjectActivity" /> <!-- 네이버지도 -->
        <meta-data
            android:name="com.naver.maps.map.CLIENT_ID"
            android:value="hzqct6qd8n" /> <!-- 네이버지도 -->
        <!-- 엑티비티 -->
        <activity android:name=".UserSetActivity" />
        <activity android:name=".ProjectRunningActivity" />
        <activity android:name=".NormalRunningActivity" />
        <activity android:name=".MissionActivity" />
        <activity android:name=".ResultActivity" />
        <activity android:name=".RunningActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignupActivity" />

        <activity android:name=".course.activity.SketchBookRunnerActivity">

        </activity>
        <activity android:name=".course.activity.GuideRunnerActivity" />

    </application>

</manifest>